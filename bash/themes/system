teal=$(tput setaf 6)
silver=$(tput setaf 7)
grey=$(tput setaf 8)
red=$(tput setaf 9)
lime=$(tput setaf 10)
yellow=$(tput setaf 11)
blue=$(tput setaf 12)
fuchsia=$(tput setaf 13)
aqua=$(tput setaf 14)
white=$(tput setaf 15)